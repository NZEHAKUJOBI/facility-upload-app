{
  "name": "facility-upload-app",
  "version": "1.0.0",
  "description": "Express app for uploading PostgreSQL database information for multiple facilities",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "backup": "cross-env NODE_ENV=production node backup.js backup",
    "backup:start": "cross-env NODE_ENV=production node backup.js start",
    "backup:list": "node backup.js list"
  },
  "keywords": [
    "express",
    "postgresql",
    "facilities",
    "upload"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "csurf": "^1.11.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "swagger-jsdoc": "^6.2.8",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
